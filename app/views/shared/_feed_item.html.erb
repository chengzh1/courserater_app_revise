<li id="<%= feed_item.id %>">
  <%= link_to gravatar_for(feed_item.user), feed_item.user %>
  <ul>
    <span class="user">
    <p style="font-family:arial;font-size:18px; color:red">
    Student's name:
    <%= link_to feed_item.user.name, feed_item.user %>
    </p>
    </span>
</ul>
<ul>
    <span class="question1">
    <p style="font-family:arial;font-size:18px; color:red">
    Based on quality of the course, <%=feed_item.user.name%> rates
    <%= feed_item.question1 %>
    </p>
    </span>
</ul>
<ul>
    <span class="question2">
    <p style="font-family:arial;font-size:18px; color:red">
    Based on how much, <%=feed_item.user.name%> like the course's teacher,<%=feed_item.user.name%> rates
    <%= feed_item.question2 %>
    </p>
    </span>
</ul>
<ul>
    <span class="question3">
     <p style="font-family:arial;font-size:18px; color:red">
    Based on how difficult the course is, <%=feed_item.user.name%> rates
    <%= feed_item.questiong3 %>
    </p>
    </span>
</ul>
    <span class="timestamp">
  
      Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
    </span>
  <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: feed_item.course_id %>
                            
  <% end %>
</li>