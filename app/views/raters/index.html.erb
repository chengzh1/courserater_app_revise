<h1>Listing raters</h1>

<table>
  <tr>
    <th>Question1</th>
    <th>Question2</th>
    <th>Questiong3</th>
    <th>Question4</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @raters.each do |rater| %>
  <tr>
    <td><%= rater.question1 %></td>
    <td><%= rater.question2 %></td>
    <td><%= rater.questiong3 %></td>
    <td><%= rater.question4 %></td>
    <td><%= rater.course_id %></td>
    <td><%= link_to 'Show', rater %></td>
    <td><%= link_to 'Edit', edit_rater_path(rater) %></td>
    <td><%= link_to 'Destroy', rater, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

